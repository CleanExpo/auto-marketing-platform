{
  "framework": {
    "name": "Auto Marketing A/B Testing Framework",
    "version": "1.0.0",
    "methodology": "frequentist_and_bayesian",
    "confidence_level": 0.95,
    "power": 0.80,
    "minimum_detectable_effect": 0.05
  },
  
  "test_library": {
    "hook_tests": [
      {
        "test_id": "HOOK_STYLE_MILLENNIALS_001",
        "name": "Humorous vs Serious Hooks - Millennials",
        "status": "ready",
        "hypothesis": "Humorous hooks will increase CTR by 15% for millennials compared to serious hooks",
        "reasoning": "Millennials respond better to authentic, entertaining content based on engagement data",
        "target_audience": {
          "age_range": "25-34",
          "persona": "creative_professional",
          "device": ["mobile", "desktop"]
        },
        "variations": {
          "control": {
            "id": "serious_hook",
            "name": "Serious Professional",
            "content": "Transform your marketing strategy with data-driven insights",
            "allocation": 50
          },
          "variant_a": {
            "id": "humorous_hook",
            "name": "Humorous Casual",
            "content": "Marketing so easy, even your boss will think you're a genius",
            "allocation": 50
          }
        },
        "metrics": {
          "primary": "click_through_rate",
          "secondary": ["engagement_rate", "share_rate", "conversion_rate"],
          "guardrail": ["bounce_rate", "unsubscribe_rate"]
        },
        "sample_size": {
          "calculated": 3100,
          "per_variant": 1550,
          "calculation_params": {
            "baseline_rate": 0.10,
            "minimum_effect": 0.15,
            "alpha": 0.05,
            "power": 0.80
          }
        },
        "duration": {
          "minimum_days": 14,
          "maximum_days": 30,
          "estimated_days": 18
        },
        "expected_outcome": {
          "lift": 15,
          "confidence": 95,
          "impact": "high"
        }
      },
      {
