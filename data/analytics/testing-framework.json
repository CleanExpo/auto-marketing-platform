  "test_configuration": {
    "traffic_allocation": {
      "default": 100,
      "high_risk": 20,
      "gradual_rollout": {
        "day_1": 5,
        "day_3": 25,
        "day_7": 50,
        "day_14": 100
      }
    },
    
    "segmentation": {
      "enabled": true,
      "segments": [
        {
          "name": "new_visitors",
          "criteria": "first_session = true"
        },
        {
          "name": "returning_visitors",
          "criteria": "session_count > 1"
        },
        {
          "name": "mobile_users",
          "criteria": "device_category = 'mobile'"
        },
        {
          "name": "high_intent",
          "criteria": "pages_viewed > 3 OR time_on_site > 180"
        }
      ]
    },
    
    "exclusion_rules": [
      {
        "rule": "employees",
        "criteria": "email_domain = 'automarketing.com'"
      },
      {
        "rule": "existing_customers",
        "criteria": "customer_status = 'active'"
      },
      {
        "rule": "bots",
        "criteria": "user_agent CONTAINS 'bot' OR 'crawler'"
      }
    ]
  },
  
  "statistical_settings": {
    "test_methodology": {
      "primary": "frequentist",
      "secondary": "bayesian",
      "use_sequential_testing": true,
      "multiple_comparison_correction": "bonferroni"
    },
    
    "stopping_rules": {
      "minimum_sample_size": true,
      "minimum_duration": true,
      "statistical_significance": true,
      "practical_significance": true,
      "harm_detection": true
    },
    
    "sample_size_calculator": {
      "formula": "n = 2 * (Z_alpha + Z_beta)^2 * p * (1-p) / delta^2",
      "defaults": {
        "alpha": 0.05,
        "power": 0.80,
        "two_tailed": true
      }
    },
    
    "bayesian_priors": {
      "conversion_rate": {
        "distribution": "beta",
        "alpha": 1,
        "beta": 1
      },
      "revenue": {
        "distribution": "gamma",
        "shape": 2,
        "scale": 50
      }
    }
  },
  
