# Platform-Specific Marketing Mastery System Environment Variables
# Copy this file to .env and fill in your actual values

# AI Service API Keys (Optional for testing)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Social Media Platform OAuth Credentials (Optional for testing)
YOUTUBE_CLIENT_ID=your_youtube_client_id
YOUTUBE_CLIENT_SECRET=your_youtube_client_secret
INSTAGRAM_CLIENT_ID=your_instagram_client_id
INSTAGRAM_CLIENT_SECRET=your_instagram_client_secret

# Security (Optional for testing)
ENCRYPTION_KEY=test_key_for_development_only
JWT_SECRET_KEY=test_jwt_secret_for_development

# Application Configuration
ENVIRONMENT=testing
DEBUG_MODE=true
LOG_LEVEL=INFO
MAX_CPU_USAGE=75