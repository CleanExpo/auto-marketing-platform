    "presentation": {
      "slide_deck": {
        "dimensions": "1920x1080",
        "format": ["PPTX", "KEY", "PDF"],
        "templates": ["pitch", "report", "training", "webinar"]
      },
      "infographic": {
        "dimensions": "800x2000",
        "format": ["PNG", "PDF"],
        "templates": ["data", "process", "timeline", "comparison"]
      }
    },
    "brand_elements": {
      "logo": {
        "formats": ["SVG", "PNG", "EPS"],
        "variants": ["full", "icon", "wordmark"],
        "colorways": ["full_color", "black", "white", "grayscale"]
      },
      "favicon": {
        "dimensions": ["16x16", "32x32", "192x192"],
        "format": ["ICO", "PNG"],
        "variants": ["standard", "dark_mode"]
      },
      "watermark": {
        "opacity": "20-30%",
        "position": ["corner", "center"],
        "format": ["PNG", "SVG"]
      }
    }
  },
  "asset_specifications": {
    "file_naming": {
      "convention": "kebab-case",
      "structure": "{platform}-{type}-{variant}-{date}",
      "example": "instagram-post-product-20240101"
    },
    "color_profiles": {
      "web": "sRGB",
      "print": "CMYK",
      "video": "Rec. 709"
    },
    "compression": {
      "jpg_quality": 85,
      "png_optimization": true,
      "webp_quality": 80
    },
    "accessibility": {
      "alt_text": "required",
      "color_contrast": "WCAG AA",
      "text_size": "minimum 14px"
    }
  },
  "production_workflow": {
    "stages": [
      {
        "name": "concept",
        "deliverables": ["mood_board", "sketches", "wireframes"]
      },
      {
        "name": "design",
        "deliverables": ["mockups", "prototypes", "style_guide"]
      },
      {
        "name": "review",
        "deliverables": ["feedback", "revisions", "approval"]
      },
      {
        "name": "production",
        "deliverables": ["final_files", "exports", "packages"]
      },
      {
        "name": "delivery",
        "deliverables": ["organized_files", "documentation", "handoff"]
      }
    ]
  }
}